{
  "private": "true",
  "name": "multidise",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ilya",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack ",
    "dev": "cross-env NODE_ENV=development webpack ",
    "format": "eslint src --cache --fix",
    "format:all": "pnpm format && pnpm format:style && prettier-package-json **/package.json --write",
    "format:style": "stylelint \"src/**/*.scss\" --cache --fix",
    "serve": "cross-env NODE_ENV=development webpack serve --open",
    "start": "npm run serve",
    "stats": "cross-env NODE_ENV=production webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "workspaces": [
    "linters/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.16.0",
    "@multidise/eslint-config": "workspace:^0.0.1",
    "@multidise/stylelint-config": "workspace:^0.0.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "eslint": "^8.22.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-sort-media-queries": "^3.11.12",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "stylelint": "^14.10.0",
    "typescript": "^4.7.4",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.5.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 3 versions"
  ]
}
